{
  "name": "thegas",
  "version": "0.0.4",
  "description": "A serverless graphql backend framework",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/haruska/thegas",
    "type": "git"
  },
  "author": "Jason Haruska <jharuska@gmail.com>",
  "license": "MIT",
  "keywords": [
    "GraphQL",
    "Serverless",
    "Lambda"
  ],
  "homepage": "https://github.com/haruska/thegas",
